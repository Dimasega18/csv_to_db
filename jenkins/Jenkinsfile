pipeline {
    agent any
    stages {
        stage('Run Python') {
            environment {
                    user = credentials('user')
                    host = credentials('host')
                    pass = credentials('password')
                    port = credentials('port')
                    dbms = 'postgresql'
                    tbname = 'data_hotel'
                    dbname = 'cobaaja'
                }
            steps {
                sh "python3 -db '${dbms}' -u '${user}' -hs '${host}' -ps '${pass}' -p '${port}' -n '${dbname}' -tbname '${tbname}'"
            }
        }
    }
    post {
        always {
            echo "Ini terus di post"
        }
    }   
}