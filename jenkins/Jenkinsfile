pipeline {
    agent any
    stages {
        stage('Run Python') {
            steps {
                sh python main.py -db postgresql -u dimas -hs localhost -ps ahayde -p 5432 -n cobaaja -csv ./Worlds_Best_50_Hotels.csv -tb top_50_hotels
            }
        }
    }
    post {
        always {
            echo "Ini terus di post"
        }
    }   
}