pipeline {
    agent any
    stages {
        stage('Run Python') {
            environment {
                    user = credentials('user')
                    host = credentials('host')
                    pass = credentials('password')
                    port = credentials('port')
                    dbms = 'postgresql'
                    tbname = 'data_hotel'
                    
                }
            steps {
                sh python3 --
            }
        }
    }
    post {
        always {
            echo "Ini terus di post"
        }
    }   
}